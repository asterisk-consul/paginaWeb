---
const {
  size = 24,
  color = "currentColor",
  strokeWidth = 2,
  class: className = "",
  ...rest
} = Astro.props;
---

<svg
  {...rest}
  width={size}
  height={size}
  viewBox="0 0 24 24"
  fill="none"
  stroke={color}
  stroke-width={strokeWidth}
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
  ><rect width="18" height="7" x="3" y="3" rx="1"></rect><rect
    width="9"
    height="7"
    x="3"
    y="14"
    rx="1"></rect><rect width="5" height="7" x="16" y="14" rx="1"></rect></svg
>
